$navbar-default-bg: #29C;
$navbar-inverse-bg: #111;
$navbar-height:100px;
$navbar-default-link-color: #222;
$btn-border-radius-base: 0px;
$btn-border-radius-large: 0px;
$grid-gutter-width:         30px;
$container-large-desktop:      (1340px + $grid-gutter-width);
$brand-primary: #87CDEF;
$btn-primary-color: #222;
$navbar-default-toggle-hover-bg:           none;
$navbar-default-toggle-icon-bar-bg:        $navbar-default-link-color;
$navbar-default-toggle-border-color:       none;
$navbar-collapse-max-height:       380px !default;
$nav-link-hover-bg: $brand-primary;
$border-radius-base: 0;
$text-color: #eee;
$link-color: $font-color;
$link-hover-color: darken($link-color, 15%);

$input-border-radius: 0;
$alert-border-radius: 0;

//import the rest, excluding any marked with !default that we've set above
@import "../vendor/bootstrap-sass/assets/stylesheets/bootstrap/variables";
$nav-link-hover-color: $gray-darker;
